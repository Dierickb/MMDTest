<% active = true; i=0; %> 
<% formulario.forEach(element => { %>
    
    <% if (active === true ) { %> 
        <div class="carousel-item active">                            
            <% active = false; %> 
        
    <% } else { %> 
        <div class="carousel-item">          
    <% } %>  
            <div class="cardBox np-b">
                <div class="card nr-b nmg-b">
                    <div class="cardTitle">
                        <div class="numbers" id="numPos">
                            <%= element.dimensions %>
                        </div>
                        <div class="cardName">

                        </div>
                    </div>


                    <div class="cardContent" id="cardContent-<%= i %> ">
                        <table class="table">
                            <thead>
                                <tr>
                                    <% for( let j = 0; j <columnHeader.length; j++ ) { %> 
                                        <th scope="col">
                                            <%= columnHeader[j] %> 
                                        </th>
                                    <% } %> 
                                </tr>
                            </thead>
                            <tbody class="dimensionItems" id="<%= element.idDimensions %> ">
                                <% let arrayQuestByDimensions = element.questions; let j=0; %> 
                                <% arrayQuestByDimensions.forEach(questionary => { %> 
                                    <tr>
                                        <% for( let k = 0; k < columnHeader.length; k++) { %> 
                                            <td scope="col" class="wd-min" 
                                                <% if(k===1) { %> 
                                                    id="<%= element.idButtonsDim %> - <%= (j+1) %> "
                                                <% } %>                                            
                                            >
                                                <% if(k===0) { %> 
                                                    <%= (j+1) %> 
                                                <% } else if ( k === 1) {%>    
                                                    <%= element.questions[j] %> 
                                                <% } else { %> 
                                                    <div class="form-check form-check-inline">
                                                        <input type="radio" class="form-check-input" 
                                                        name="<%= element.idButtonsDim %> - <%= j %> "
                                                        id="<%=element.idButtonsDim %> - <%= (j+1) %>  - <%= (k-1) %> " 
                                                        value="<%= (k-1) %> "> 
                                                        <label for="inlineCheckbox<%= (k+1) %> "></label>
                                                    </div>                                                    
                                                <% } %> 

                                            </td>
                                        <% } %> 
                                    </tr>
                                <% j=j+1; }); %>
                            </tbody>
                        </table>                    
                    </div>
                </div>
            </div>
        </div>
    
<% }); %>


