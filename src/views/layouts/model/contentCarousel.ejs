<% active = true; i=0; %> 
<% for (let j = 0; j < formulario.length; j++) { %>
    
    <% if (active === true ) { %> 
        <div class="carousel-item active">                            
            <% active = false; %> 
        
    <% } else { %> 
        <div class="carousel-item">          
    <% } %>  
            <div class="cardBox np-b">
                <div class="card nr-b nmg-b">
                    <div class="cardTitle">
                        <div class="numbers" id="numPos">
                            <%= formulario.process[j] %>
                        </div>
                        <div class="cardName">   
                            <%= formulario.process[j] %> 
                        </div>
                    </div>

                    <div class="cardContent" id="cardContent-<%= j+1 %> ">
                        <table class="table">
                            <thead>
                                <tr>
                                    <% for( let j = 0; j <columnHeader.length; j++ ) { %> 
                                        <th scope="col">
                                            <%= columnHeader[j] %> 
                                        </th>
                                    <% } %> 
                                </tr>
                            </thead>
                            <tbody class="dimensionItems" id="<%= formulario.idSector[j] %> ">
                                
                            </tbody>
                        </table>                    
                    </div>
                </div>
            </div>
        </div>
    
<% }; %>


