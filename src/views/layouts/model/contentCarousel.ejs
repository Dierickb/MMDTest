<% active = true; j=0;%> 

<% for (property in axesByProcess) { %>

    <%  i=0; %> 
    <% if (active === true ) { %> 
        <div class="carousel-item active">                            
            <% active = false; %> 
        
    <% } else { %> 
        <div class="carousel-item">          
    <% } %>  
            <div class="cardBox np-b">
                <div class="card nr-b nmg-b">
                    <div class="cardTitle">
                        <div class="numbers" id="numPos">
                            <%= process.process[j] %>
                        </div>
                        <div class="cardName">   
                            <%= process.process[j] %> 
                        </div>
                    </div>

                    <div class="cardContent" id="cardContent-<%= j+1 %> ">
                        <table class="table">
                            <thead>
                                <tr>
                                    <% for( let j = 0; j <columnHeader.length; j++ ) { %> 
                                        <th scope="col">
                                            <%= columnHeader[j] %> 
                                        </th>
                                    <% } %> 
                                </tr>
                            </thead>
                            <tbody class="dimensionItems" id="<%= process.idSector[j] %> ">
                                <% for( let j = 0; j < axesByProcess[property].length; j++ ) { %> 
                                    <tr>
                                        <td scope="col" class="wd-min">
                                            <%= j+1  %> 
                                        </td>
                                        <td scope="col" class="wd-min" >
                                            <%= axesByProcess[property][j]  %> 
                                        </td>
                                        <% for( let j = 0; j < 5; j++ ) { %> 
                                            <td scope="col" class="wd-min">
                                                <div class="form-check">
                                                    <input class="form-check-input" type="radio" name="flexRadioDefault" id="flexRadioDefault2" checked>
                                                    <label class="form-check-label" for="flexRadioDefault2">
                                                      
                                                    </label>
                                                </div>                                                            
                                            </td>                         
                                        <% } %>                        
                                    </tr>
                                <% } %>     
                            </tbody>
                        </table>                    
                    </div>
                </div>
            </div>
        </div>
    
<% j=j+1;}; %>


