<% for (property in dimension) { %>

    <%  i=0; %> 
    <% if (active === true ) { %> 
        <div class="carousel-item active">                            
            <% active = false; %> 
        
    <% } else { %> 
        <div class="carousel-item">          
    <% } %>  
            <div class="cardBox np-b">
                <div class="card nr-b nmg-b">
                    <div class="cardTitle">
                        <div class="numbers" id="numPos">
                            <%= dimension[property].dimension  %> 
                        </div>
                        <div class="cardName"> 
                            Tener en cuenta que 1 es muy bajo y 5 es muy alto  
                        </div>
                    </div>

                    <div class="cardContent" id="cardContent-<%= j+1 %> ">
                        <table class="table">
                            <thead>
                                <tr>
                                    <% for( let j = 0; j <columnHeader.length; j++ ) { %> 
                                        <th scope="col">
                                            <%= columnHeader[j] %> 
                                        </th>
                                    <% } %> 
                                </tr>
                            </thead>
                            <tbody class="dimensionItems">
                                <%- include('./ourTest') %>
                            </tbody>
                        </table>                    
                    </div>
                </div>
            </div>
        </div>
    
<% j=j+1;}; %>


