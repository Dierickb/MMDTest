<% let i = 0; let active = true;%> 
<% for (let j = 0; j < process.length; j++) { %>
    <% if (active === true ) { %> 
                
        <% if (url === "/MinTicTest" ) { %> 
            <button type="button" title="<%= process.process[j] %> "
        <% } else { %> 
            <button type="button" title="<%= process[j] %> "
        <% } %>        
                data-bs-target="#carouselExampleDark" data-bs-slide-to="<%= i %>"
                aria-label="Slide <%= (i+1) %> " aria-current="true" class="active"
            ></button>


    <% } else { %> 

        <% if (url === "/MinTicTest" ) { %> 
            <button type="button" title="<%= process.process[j] %> "
        <% } else { %> 
            <button type="button" title="<%= process[j] %> "
        <% } %>            
                data-bs-target="#carouselExampleDark" data-bs-slide-to="<%= i %>"
                aria-label="Slide <%= (i+1) %> " aria-current="true"
            ></button>
    <% } %> 
    
    <% i=i+1; active=false; %>
<% }; %>